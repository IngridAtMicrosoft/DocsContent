{
  "build": {
    "content": [
      {
        "src": ".",
        "dest": ".",
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.mmd",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.mmd",
          "**/*.yml"
        ],
        "src" : "avi",
        "group" : "avi", 
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.mmd",
          "**/*.yml"
        ],
        "src" : "aviarc",
        "group" : "aviarc", 
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md"
        ]
      }
    ],
    "groups": {
      "avi": {
        "dest": "avi",
        "moniker_range": "azure-video-indexer-1"
      },
      "aviarc": {
        "dest": "aviarc",
        "moniker_range": "azure-video-indexer-arc-1"
      }
    },
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**"
        ]
      },
      {
        "src": "avi",
        "group": "avi",
        "dest": "avi",
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**"
        ]
      },
      {
        "src": "aviarc",
        "group": "aviarc",
        "dest": "aviarc",
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/singlesource/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "None",
      "uhfHeaderId" : "Azure"
    },
    "fileMetadata": {},
    "template": ["default","DocFx.Plugins.PlantUml/template","modern"],
    "dest": "singlesource",
    "markdownEngineName": "markdig",
    "markdownEngineProperties" : {
      "plantUml" : {
        "outputFormat" : "svg",
        "remoteUrl" : "https://www.plantuml.com/plantuml",
        "renderingMode" : "remote"
      }
    }
  }
}
