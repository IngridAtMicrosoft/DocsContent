{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.mmd",
          "**/*.yml",
          "**/*.html"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "group": "group1",
        "src": "folder1"
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "group": "group2",
        "src": "folder2"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**"
        ]
      },
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg"
        ],
        "group": "group1",
        "src" : "folder1"
      },
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg"
        ],
        "group": "group2",
        "src" : "folder2"
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/singlesource/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "None"
    },
    "fileMetadata": {},
    "template": ["default","DocFx.Plugins.PlantUml/template"],
    "dest": "singlesource",
    "groups": {
      "group1": {
        "dest": "folder1",
        "moniker-range": "netcore-1.0"
      },
      "group2": {
        "dest": "folder2",
        "moniker-range": "netcore-2.0"
      }
    },
    "markdownEngineName": "markdig"
  }
}
